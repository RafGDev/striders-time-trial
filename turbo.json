{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^generate", "^build"],
      "cache": false,
      "persistent": true
    },
    "generate": {
      "outputs": ["src/generated/**"]
    },
    "migrate:dev": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "lint": {},
    "test": {},
    "test:e2e": {
      "dependsOn": ["^build", "api#build"],
      "cache": false
    }
  }
}
